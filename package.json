{
  "name": "@hja/react-calendar",
  "private": false,
  "version": "0.0.1",
  "description": "A customizable React calendar component by hja.",
  "type": "module",
  "main": "./dist/react-calendar.umd.js",
  "module": "./dist/react-calendar.es.js",
  "types": "./dist/src/index.d.ts",
  "style": "./dist/react-calendar.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "import": "./dist/react-calendar.es.js",
      "require": "./dist/react-calendar.umd.js",
      "types": "./dist/src/index.d.ts",
      "style": "./dist/react-calendar.css"
    },
    "./index.css": "./dist/react-calendar.css"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "react",
    "calendar",
    "component",
    "datepicker"
  ],
  "author": "HanJeongA <mongndam@naver.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanjeonga/react-calendar"
  }
}
